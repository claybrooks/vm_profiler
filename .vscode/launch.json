    "version": "0.2.0",
    "configurations": [
        {
            "name": "RunAllTests.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/RunTests.py",
            "args": [
                "-o", "${workspaceFolder}/Output/$USER/",
                "-c",
                "-r",
                "-p", "8",
                "-t", "3",
                "-a"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "RunCPUTests.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/RunTests.py",
            "args": [
                "-o", "${workspaceFolder}/Output/$USER/",
                "--class", "cpu",
                "-c",
                "-r",
                "-p", "8",
                "-t", "3",
                "-a"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "RunMemoryTests.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/RunTests.py",
            "args": [
                "-o", "${workspaceFolder}/Output/$USER/",
                "--class", "memory",
                "-c",
                "-r",
                "-p", "8",
                "-t", "3",
                "-a"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "AnalyzeResults.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/RunTests.py",
            "args": [
                "-o", "${workspaceFolder}/Output/$USER/",
                "-a"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "AnalyzeCPUResults.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/RunTests.py",
            "args": [
                "-o", "${workspaceFolder}/Output/$USER/",
                "--class", "cpu",
                "-a"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "AnalyzeMemoryResults.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/RunTests.py",
            "args": [
                "-o", "${workspaceFolder}/Output/$USER/",
                "--class", "memory",
                "-a"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "ListStressors.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/RunTests.py",
            "args": ["-l", "-o", "${workspaceFolder}/Output/$USER/"],
            "console": "integratedTerminal"
        }
    ]
}