    "version": "0.2.0",
    "configurations": [
        {
            "name": "RunTests.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/RunTests.py",
            "args": [
                "-o", "${workspaceFolder}/Output/$USER/",
                "-c",
                "-t", "10"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "ListStressors.py",
            "type": "python",
            "request": "launch",
